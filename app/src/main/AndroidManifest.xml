<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanadu">
    <!-- /*인터넷 권한 */ -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ui.game.GameActivity"></activity>
        <activity android:name=".ui.mypage.MyPageActivity" />
        <activity android:name=".ui.schedule.ScheduleActivity" />
        <activity android:name=".ui.login.SignupActivity" />
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.graph_detail.DetailActivity" />
        <activity android:name=".ui.MainActivity">

        </activity>
    </application>

</manifest>